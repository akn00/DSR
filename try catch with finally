<script>
function tryCatchFinally(){
    let result;
    try {
        console.log('inside try block : ');
        //x=10;
        result = x / 10;
        console.log(result);
    } catch (error) {
        console.log('inside catch block : ')
        console.log(error.name +' : ' +error.message);
    }
    finally{
        console.log('This is finally block');
    }
}
</script>
</body>
